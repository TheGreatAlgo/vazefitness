doctype html
html(lang='en')
  head
    title
      | Vaze Fitness Login
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    link(rel='shortcut icon', type='image/x-icon', href='images/favicon.ico')
    link(rel='stylesheet', type='text/css', href='styles/styles.css')
    style.
      * {
      box-sizing: border-box;
      }
      .header a{
      float: left;
      display: block;
      color: white;
      text-align: center;
      padding: 14px 20px;
      text-decoration: none;
      }
      .textboxconnect p {
      text-align: center;
      display: block;
      font-family: Helvetica;
      font-size: 22px;
      font-style: normal;
      font-weight: bold;
      text-decoration: none;
      text-transform: none;
      color: #000000;
      position: relative;
      background-color: #ffffff;
      padding: 0% 0 0;
      margin-left: auto;
      margin-right: auto;
      width: 100%;
      }
      .column {
      float: left;
      width: 50%;
      }
      /* Clear floats after the columns */
      .row:after {
      content: "";
      display: table;
      clear: both;
      }
  body
    .header
      div
        img(src='../images/VAZEFitness.png', style='vertical-align:middle;', width='480', height='80', alt='VazeLogo')
        img(src='../images/chainlinkPowered.png', style='vertical-align:middle; float:right; padding-right: 10px;', width='450', height='80', alt='ChainlinkLogo')
    .navbar
      a.left(href='./dashboard') Dashboard
      a.left(href='./wallet') Wallet
      a.left(href='./deploy') Deploy
      a.left(href='./deployedContracts') Contracts
      a.right(href='./logout') Logout
